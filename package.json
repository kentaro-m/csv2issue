  "version": "1.0.2",
  "author": "Kentaro Matsushita @kentaro-m",
  "bin": {
    "mkissue": "./bin/run"
  },
  "bugs": "https://github.com/kentaro-m/mkissue/issues",
  "dependencies": {
    "@oclif/command": "^1.5.13",
