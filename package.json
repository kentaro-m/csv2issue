    "@oclif/plugin-help": "^2.1.6",
    "@octokit/rest": "^16.27.0",
    "@types/csv-parse": "^1.2.2",
    "@types/node": "^12.0.2",
    "cli-ux": "^5.2.1",
    "csv-parse": "^4.4.1",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.2",
